<div class="container">
    <div class="col-md-4">
        <h2>Shipment</h2>
        <div class="table-responsive">
            <table class="table table-condensed table-bordered table-hover table-striped">
                <thead>
                    <tr class="btn-primary">
                        <th><b>Vehicle Code</b></th>
                        <th><b>Order Code</b></th>
                        <th><b>Status</b></th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>LZN-3597</td>
                        <td>213</td>
                        <td>Open</td>
                    </tr>
                    <tr>
                        <td>AEE-4511</td>
                        <td>11</td>
                        <td>Queued</td>
                    </tr>
                    <tr>
                        <td>LEA-9331</td>
                        <td>654</td>
                        <td>Checked-In</td>
                    </tr>
                    <tr>
                        <td>KSA-1231</td>
                        <td>911</td>
                        <td>Manual Entry</td>
                    </tr>
                    <tr>
                        <td>MNL-4272</td>
                        <td>123</td>
                        <td>Closed</td>
                    </tr>
                </tbody>
            </table>
            <ul class="pagination pull-right">
                <li class="disabled"><a href="#"><span class="glyphicon glyphicon-chevron-left"></span></a></li>
                <li class="active"><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">4</a></li>
                <li><a href="#">5</a></li>
                <li><a href="#"><span class="glyphicon glyphicon-chevron-right"></span></a></li>
            </ul>
        </div>
        <div class="container">
            <h2>Compartment Planning</h2>
            <table class="table table-condensed table-bordered table-hover table-striped">
                <thead>
                    <tr class="btn-primary">
                        <th><b>Compartment</b></th>
                        <th><b>Order Code</b></th>
                        <th><b>Product</b></th>
                        <th><b>Capacity</b></th>
                        <th><b>Ordered QTY</b></th>
                        <th><b>Planned QTY</b></th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1</td>
                        <td>211</td>
                        <td>Condensate</td>
                        <td>3000</td>
                        <td>9000</td>
                        <td>3000</td>
                    </tr>
                    <tr>
                        <td>2</td>
                        <td>211</td>
                        <td>Condensate</td>
                        <td>3000</td>
                        <td>9000</td>
                        <td>3000</td>
                    </tr>
                    <tr>
                        <td>3</td>
                        <td>211</td>
                        <td>Condensate</td>
                        <td>3000</td>
                        <td>9000</td>
                        <td>3000</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-md-8">
        <div class="form-group ">
            <div class="btn-group col-md-offset-4">
                <button type="button" class="btn btn-primary">Save</button>
                <button type="button" class="btn btn-primary">Close</button>
                <button type="button" class="btn btn-primary">Print LoadSlip</button>
                <button type="button" class="btn btn-primary">Print BoL</button>
            </div>
        </div>
        <form class="form-horizontal">
            <div class="form-group">
                <label for="customer" class="col-md-4 control-label">Customer</label>
                <div class="col-md-4">
                    <input type="text" class="form-control" id="customer" readonly="readonly" value="PSO" />
                </div>
            </div>
            <div class="form-group">
                <label for="vehCode" class="col-md-4 control-label">Vehicle Code</label>
                <div class="col-md-4">
                    <input type="text" class="form-control" id="vehCode" readonly="readonly" value="LZN-5387" />
                </div>
            </div>
            <div class="form-group">
                <label for="drvName" class="col-md-4 control-label">Driver Name</label>
                <div class="col-md-4">
                    <input type="text" class="form-control" id="drvName" readonly="readonly" value="Amjad Jamal" />
                </div>
            </div>
            <div class="form-group">
                <label for="shipDate" class="col-md-4 control-label">Date Of Shipment</label>
                <div class="col-md-4">
                    <input type="text" class="form-control" id="shipDate" placeholder="Date/Time" />
                </div>
            </div>
            <div class="form-group">
                <label for="baySelect" class="col-md-4 control-label">BAY</label>
                <div class="col-md-4">
                    <select id="baySelect" class="form-control">
                        <option>BAY-01</option>
                        <option>BAY-02</option>
                        <option>BAY-03</option>
                        <option>BAY-04</option>
                        <option>BAY-05</option>
                        <option>BAY-06</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label for="rfidSelect" class="col-md-4 control-label">RFID Card No</label>
                <div class="col-md-4">
                    <select id="baySelect" class="form-control">
                        <option>Card-01</option>
                        <option>Card-02</option>
                        <option>Card-03</option>
                        <option>Card-04</option>
                        <option>Card-05</option>
                        <option>Card-06</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label for="" class="col-md-4 control-label">Manual Entry</label>
                <div class="col-md-4">
                    <input type="checkbox" />
                </div>
            </div>
        </form>
    </div>
</div>

